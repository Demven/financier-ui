{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "EXPO_PUBLIC_API_BASE_URL"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "environment": "preview",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "EXPO_PUBLIC_API_BASE_URL"
      }
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "EXPO_PUBLIC_API_BASE_URL"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
